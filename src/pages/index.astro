---
import Layout from '../layouts/Layout.astro';
const title = 'Inicio';
import { getSession } from 'auth-astro/server';;
import type { Sesion } from '../types/types';
const sesion = await getSession(Astro.request);

---

<Layout title={title} pathname={Astro.url.pathname} sesion={sesion as Sesion}>
        <section class="w-full h-full flex flex-col justify-center items-center">
            <h1 class="font-semibold text-2xl ">
                Bienvenido a la p√°gina de inicio
            </h1>
            <p class="text-zinc-500 dark:text-zinc-400 text-sm mb-8 w-full"></p>
        </section>
</Layout>
